<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="hypelabs.com.hypepubsub">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SubscriptionsListActivity">
                  android:parentActivityName=".MainActivity" >
                  android:label="Own Subscriptions"
        </activity>
        <activity android:name=".HypeDevicesListActivity">
            android:parentActivityName=".MainActivity" >
            android:label="Hype Devices Found"
        </activity>
        <activity android:name=".ServiceManagersListActivity">
            android:parentActivityName=".MainActivity" >
            android:label="Managed Services"
        </activity>
    </application>

</manifest>